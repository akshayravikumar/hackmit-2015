{
  "manifest_version": 2,
  "name": "Music",
  "version": "0.1",
  "content_scripts": [
	  {
	    "matches": [
	      "<all_urls>"
	    ],
	    "js": ["soundcloud.js", "content.js"]
	  }
	],
	"browser_action": {
	  "default_icon": "icon.png",
      "default_popup": "popup.html"
	},
	"permissions": [
        "http://www.domain.com/*"
    ]
}

